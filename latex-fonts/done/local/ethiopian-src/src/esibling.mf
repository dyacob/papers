x6=x1r; y6=y1; lft_wing(6,u,.025h);
x9=x0; y9=y0; rt_wing(9,0,0);

x20=x0+2u; x21=x20; y20=y21=y0-.175h; lft_wing(20,2.5u,.015h); 
