% Program name:---------------- dsibling.mf
cmchar "The letter dsibling";
beginchar(oct "144",10u#,.5[cap_height#,x_height#],paren_depth#);
adjust_fit(-.5u#,5u#);
numeric x_bar;

% THE RIGHT TOP ARC
pickup fine.nib; pos0(vair,270); penpos1(stem,180); penpos2(stem,180);
penpos2'(stem,180); pos3(vair,105);

x1=w-4.5u; x2r=w-7u;  x2'r=x2r+.2u; x3=w-3.5u; x0=w-2.5u;
