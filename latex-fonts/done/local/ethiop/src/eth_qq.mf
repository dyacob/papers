x10=x0l; x11=x10+3u; x11'=x11+.25u; x6=x0; x7=x0r; x8=x7-3u; x9=x0; x8'=x8;
y10=.575h; y9=.525h; y7=y9-.35h; y6=y10-.3h;
y11=y6+.4(y10-y6);  y11'=y6+.5(y10-y6); y8'=y7+.3(y9-y7); y8=y7+.5(y9-y7);
